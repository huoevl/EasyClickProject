{
    "compilerOptions": {
      "target": "ES5",                // 将 JavaScript 目标版本设置为 ES5
      "module": "CommonJS",           // 使用 CommonJS 模块系统
      "strict": true,                 // 启用所有严格类型检查选项
      "esModuleInterop": true,        // 启用对 ES 模块导入的互操作性
      "skipLibCheck": true,           // 跳过库文件的类型检查
      "forceConsistentCasingInFileNames": true // 强制文件名使用一致的大小写
    }
  }